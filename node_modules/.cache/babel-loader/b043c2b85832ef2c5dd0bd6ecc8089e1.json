AA,KAAA,cAAA,KAAA,IAAA;AACA,KAHA;AAIA,IAAA,gBAAA,EAAA,4BAAA;AACA,UAAA,UAAA,GAAA,IAAA,CAAA,UAAA;AAAA,UAAA,MAAA,GAAA,IAAA,CAAA,MAAA;AACA,aAAA,UAAA,CAAA,MAAA,CAAA,UAAA,SAAA,EAAA;AACA,eAAA,SAAA,CAAA,IAAA,CAAA,WAAA,GAAA,OAAA,CAAA,MAAA,CAAA,WAAA,EAAA,MAAA,CAAA,CAAA;AACA,OAFA,CAAA;AAGA;AATA,GAzBA;AAoCA,EAAA,OApCA,qBAoCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,cAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AADA;AAAA,qBAEA,MAAA,CAAA,eAAA,EAFA;;AAAA;AAGA,cAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA;AAxCA,CAAA","sourcesContent":["<template>\n    <div id=\"app\">\n        <app-header :changeSearch=\"changeSearch\"/>\n\n        <div class=\"container\">\n            <h1 class=\"pt-3 pb-3\">Персонажи Marvel</h1>\n\n            <app-modal :character=\"character\"/>\n            <spinner v-if=\"loading\"/>\n            <div v-else class=\"row\">\n                <h5 v-if=\"!searchCharacters.length\">Ничего не найдено</h5>\n                <div v-else\n                     v-for=\"(el, idx) in searchCharacters\"\n                     :key=\"el.id\"\n                     class=\"card mb-3 col-sm-12 col-md-6 col-lg-4\">\n                    <div class=\"row g-0\">\n                        <div class=\"col-4\">\n                            <img\n                                    style=\"max-width: 100%;\"\n                                    :src=\"el.thumbnail\"\n                                    :alt=\"el.name\"\n                            >\n                        </div>\n                        <div class=\"col-8\">\n                            <div class=\"card-body\">\n                                <h5 class=\"card-title\">{{el.name}}</h5>\n\n                                <button\n                                        type=\"button\"\n                                        class=\"btn btn-secondary btn-sm\"\n                                        data-bs-toggle=\"modal\"\n                                        data-bs-target=\"#exampleModal\"\n                                        @click=\"characterIndex = idx\"\n                                >\n                                    Подробнее\n                                </button>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n\n    </div>\n</template>\n\n<script>\n    import Spinner from \"./components/Spinner\";\n    import AppModal from \"./components/AppModal\";\n    import AppHeader from \"./components/AppHeader\";\n\n    export default {\n        name: 'App',\n        components: {\n            AppHeader,\n            AppModal,\n            Spinner,\n        },\n        data() {\n            return {\n                loading: false,\n                characters: [],\n                characterIndex: 0,\n                search: '',\n            }\n        },\n        methods: {\n            fetchCharacters: function () {\n                return fetch(`https://netology-api-marvel.herokuapp.com/characters`)\n                    .then(response => response.json())\n                    .then(json => this.characters = json)\n            },\n            changeSearch: function (value) {\n                this.search = value\n            }\n        },\n        computed: {\n            character: function () {\n                return this.searchCharacters[this.characterIndex] || null\n            },\n            searchCharacters: function () {\n                const {characters, search} = this;\n                return characters.filter((character) => {\n                    return character.name.toLowerCase().indexOf(search.toLowerCase()) !== -1\n                })\n            },\n        },\n        async mounted() {\n            this.loading = true\n            await this.fetchCharacters()\n            this.loading = false\n        }\n    }\n</script>\n\n<style>\n\n</style>\n"],"sourceRoot":"src"}]}